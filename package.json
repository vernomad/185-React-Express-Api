{
  "name": "185-react-express-api",
  "version": "1.0.0",
  "description": "version of 185",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "npm run build --prefix clients",
    "backend": "nodemon src/index.ts",
    "serve": "npx kill-port 3500 && node dist/index.js",
    "client": "npm run dev --prefix clients",
    "dev": "concurrently \"npm run backend\" \"npm run client\"",
    "start": "npm run clean && npm run build && concurrently \"npm run serve\" \"npm run client:serve\"",
    "client:serve": "npm run preview --prefix clients",
    "clean": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lambda-rate-limiter": "^3.0.2",
    "@types/node": "^22.9.0",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "lambda-rate-limiter": "^4.0.0",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.0",
    "react-tooltip": "^5.28.0",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  }
}
